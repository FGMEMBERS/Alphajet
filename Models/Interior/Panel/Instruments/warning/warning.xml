<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>warning.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>barre</object-name>

    <object-name>lampes</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>lampes</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>lampes</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

</PropertyList>

