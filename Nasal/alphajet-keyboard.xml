<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

   <key n="68">
    <name>D</name>
    <desc>Passenger door</desc>
    <binding>
     <command>nasal</command>
     <script>alphajet.doorsystem.passengerexport();</script>
    </binding>
   </key>

   <key n="100">
    <name>d</name>
    <desc>Crew door</desc>
    <binding>
     <command>nasal</command>
     <script>alphajet.doorsystem.crewexport();</script>
    </binding>
   </key>

  <key n="83">
    <name>S</name>
    <desc>Start smoke</desc>
    <binding>
      <command>property-assign</command>
      <property>sim/multiplay/generic/int[6]</property>
      <value>true</value>
    </binding>
  </key>

  <key n="117">
    <name>u</name>
    <desc>Stop smoke</desc>
    <binding>
      <command>property-assign</command>
      <property>sim/multiplay/generic/int[6]</property>
      <value>false</value>
    </binding>
  </key>

</PropertyList>

